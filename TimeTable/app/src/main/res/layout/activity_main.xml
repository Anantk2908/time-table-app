<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <FrameLayout
        android:id="@+id/frame"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>


    <android.support.v4.view.ViewPager
        android:id="@+id/viewpager_main"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/tabs_main"/>



    <com.google.android.material.appbar.AppBarLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:theme="@style/AppTheme.AppBarOverlay">

        <TextView
            android:id="@+id/title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:minHeight="?actionBarSize"
            android:padding="@dimen/appbar_padding"
            android:text="@string/app_name"
            android:textAppearance="@style/TextAppearance.Widget.AppCompat.Toolbar.Title" />

        <com.google.android.material.tabs.TabLayout
            android:id="@+id/tabs"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="?attr/colorPrimary" />
    </com.google.android.material.appbar.AppBarLayout>

    <com.google.android.material.tabs.TabLayout
        android:id="@+id/tabs"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="?attr/colorPrimary" />



    <androidx.viewpager.widget.ViewPager
        android:id="@+id/view_pager"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_behavior="@string/appbar_scrolling_view_behavior" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:id="@+id/cul"
        android:layout_gravity="end|bottom"
        android:layout_marginBottom="330dp"
        android:layout_marginRight="25dp"
        app:fabSize="mini"
        app:elevation="6dp"
        app:pressedTranslationZ="12dp"
        android:visibility="invisible"
        />
    <TextView android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:text="CulEdu"
        android:elevation="6dp"
        android:textSize="10dp"
        android:textColor="#fff"
        app:layout_anchor="@id/cul"
        app:layout_anchorGravity="center"/>
    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:id="@+id/d_math"
        android:layout_gravity="end|bottom"
        android:layout_marginBottom="280dp"
        android:layout_marginRight="25dp"
        app:fabSize="mini"
        app:elevation="6dp"
        app:pressedTranslationZ="12dp"
        android:visibility="invisible"

        />
    <TextView android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:text="DMath"
        android:elevation="6dp"
        android:textSize="10dp"
        android:textColor="#fff"
        app:layout_anchor="@id/d_math"
        app:layout_anchorGravity="center"/>



    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:id="@+id/lin_al"
        android:layout_gravity="end|bottom"
        android:layout_marginBottom="230dp"
        android:layout_marginRight="25dp"
        app:fabSize="mini"
        app:elevation="6dp"
        app:pressedTranslationZ="12dp"
        android:visibility="invisible"

        />
    <TextView android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:text="LinAl"
        android:elevation="6dp"
        android:textSize="14dp"
        android:textColor="#fff"
        app:layout_anchor="@id/lin_al"
        app:layout_anchorGravity="center"/>



    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:id="@+id/c_prog"
        android:layout_gravity="end|bottom"
        android:layout_marginBottom="180dp"
        android:layout_marginRight="25dp"
        app:fabSize="mini"
        app:elevation="6dp"
        app:pressedTranslationZ="12dp"
        android:visibility="invisible"

        />
    <TextView android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:text="C Prog"
        android:elevation="6dp"
        android:textSize="10dp"
        android:textColor="#fff"
        app:layout_anchor="@id/c_prog"
        app:layout_anchorGravity="center"/>



    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:id="@+id/eee"
        android:layout_gravity="end|bottom"
        android:layout_marginBottom="130dp"
        android:layout_marginRight="25dp"
        app:fabSize="mini"
        app:elevation="6dp"
        app:pressedTranslationZ="12dp"
        android:visibility="invisible"

        />
    <TextView android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:text="EEE"
        android:elevation="6dp"
        android:textSize="18dp"
        android:textColor="#fff"
        app:layout_anchor="@id/eee"
        app:layout_anchorGravity="center"/>



    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:id="@+id/ds"
        android:layout_gravity="end|bottom"
        android:layout_marginBottom="80dp"
        android:layout_marginRight="25dp"
        app:fabSize="mini"
        app:elevation="6dp"
        app:pressedTranslationZ="12dp"
        android:visibility="invisible"

        />
    <TextView android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:text="DS"
        android:elevation="6dp"
        android:textSize="18dp"
        android:textColor="#fff"
        app:layout_anchor="@id/ds"
        app:layout_anchorGravity="center"/>



    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/fab"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|end"
        android:layout_margin="@dimen/fab_margin"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        app:backgroundTint="#000000"
        app:fabCustomSize="64dp"
        app:rippleColor="@color/colorPrimary"
        app:pressedTranslationZ="12dp"
        android:src="@drawable/ic_add_black_24dp"/>
</androidx.coordinatorlayout.widget.CoordinatorLayout>